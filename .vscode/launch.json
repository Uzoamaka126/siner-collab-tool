    "version": "0.2.0",
    "configurations": [
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Launch with NPM",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "command": "npm run start",
           "program": "${workspaceFolder}/api/src/config/index.ts",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ],
            "envFile": "${workspaceFolder}/api/.env"
        }
    ]
}